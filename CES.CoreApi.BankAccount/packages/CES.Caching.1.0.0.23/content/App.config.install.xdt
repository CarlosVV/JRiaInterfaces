<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

	<appSettings  xdt:Transform="InsertIfMissing">
			
		<add key="redisConnection" value="devappfab1.riadev.local:6379,devappfab2.riadev.local:6379,abortConnect=false" xdt:Transform="InsertIfMissing"  xdt:Locator="Match(key)"/>
		<add key="redisCacheTimeout" value="0.00:15:00" xdt:Transform="InsertIfMissing"  xdt:Locator="Match(key)"/>
