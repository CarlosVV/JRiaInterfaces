  <!--
    isPerformanceLogEnabled - defines whether performance log enabled
    performanceLogThreshold - threshold in milliseconds, after which system logs invocation, 0 means log all calls 
    isPerformanceLogAsynchronous - defines whether performance log is asynchronous
  -->
  <performanceLog isPerformanceLogEnabled="true" performanceLogThreshold="0" isPerformanceLogAsynchronous="true" />
  <!--
    isDatabasePerformanceLogEnabled - defines whether database performance log enabled
    databasePerformanceLogThreshold - threshold in milliseconds, after which system logs invocation, 0 means log all calls 
    isDatabasePerformanceLogAsynchronous - defines whether database performance log is asynchronous
  -->
  <databasePerformanceLog isDatabasePerformanceLogEnabled="true" databasePerformanceLogThreshold="0" isDatabasePerformanceLogAsynchronous="true" />
  <!--
    isTraceLogEnabled - defines whether trace log enabled
    isTraceLogAsynchronous - defines whether trace log is asynchronous
  -->
  <traceLog isTraceLogEnabled="true" isTraceLogAsynchronous="true" isRequestLoggingEnabled="true" isResponseLoggingEnabled="true" />
  <log4net>
    <root>
      <!-- Levels: OFF, DEBUG, INFO, NOTICE, WARN, ERROR, FATAL-->
      <level value="INFO" />
      <!--
        Log level priority in descending order:
      
        FATAL = 1  show  log -> FATAL 
        ERROR = 2  show  log -> FATAL ERROR 
        WARN =  3  show  log -> FATAL ERROR WARN 
        NOTICE = 4 show  log -> FATAL ERROR WARN NOTICE 
        INFO =  5  show  log -> FATAL ERROR WARN NOTICE INFO 
        DEBUG = 6  show  log -> FATAL ERROR WARN NOTICE INFO DEBUG
      -->
      <!--To write error log in file -->
      <appender-ref ref="ErrorLogFileAppender" />
      <!--To write error log to grey log -->
      <appender-ref ref="ErrorLogUdpAppender" />
      <!--To write trace log in file -->
      <appender-ref ref="TraceLogFileAppender" />
      <!--To write trace log to grey log -->
      <appender-ref ref="TraceLogUdpAppender" />
      <!--To write application performance messages to file -->
      <appender-ref ref="PerformanceLogFileAppender" />
      <!--To write application performance messages to grey log -->
      <appender-ref ref="PerformanceLogUdpAppender" />
      <!--To write application database performance messages to file -->
      <appender-ref ref="DatabasePerformanceLogFileAppender" />
      <!--To write application database performance messages to grey log -->
      <appender-ref ref="DatabasePerformanceLogUdpAppender" />
    </root>
    <appender name="ErrorLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\logs\PayoutService\CoreApi_PayoutService_Error_%date{yyyy-MM-dd}.txt" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%newline%date - %m%newline" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="ERROR" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="ErrorLogUdpAppender" type="gelf4net.Appender.GelfUdpAppender, Gelf4net">
      <remoteAddress value="10.30.108.38" />
      <remotePort value="12200" />
      <layout type="gelf4net.Layout.GelfLayout, Gelf4net">
        <param name="AdditionalFields" value="app:CoreAPI,service:Payout,logtype:ErrorLog,date:%date,thread:%thread,level:%level,logger:%logger" />
        <param name="ConversionPattern" value="%message" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="ERROR" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="TraceLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\logs\PayoutService\CoreApi_PayoutService_Trace_%date{yyyy-MM-dd}.txt" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date %7level [%thread] (%r ms) - %message%newline" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="INFO" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="TraceLogUdpAppender" type="gelf4net.Appender.GelfUdpAppender, Gelf4net">
      <remoteAddress value="10.30.108.38" />
      <remotePort value="12200" />
      <threshold value="ON" />
      <layout type="gelf4net.Layout.GelfLayout, Gelf4net">
        <param name="AdditionalFields" value="app:CoreAPI,service:Payout,logtype:TraceLog,date:%date,thread:%thread,level:%level,logger:%logger" />
        <param name="ConversionPattern" value="%message" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="INFO" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="PerformanceLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\logs\PayoutService\CoreApi_PayoutService_Performance_%date{yyyy-MM-dd}.txt" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%m%n" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="WARN" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="PerformanceLogUdpAppender" type="gelf4net.Appender.GelfUdpAppender, Gelf4net">
      <remoteAddress value="10.30.108.38" />
      <remotePort value="12200" />
      <layout type="gelf4net.Layout.GelfLayout, Gelf4net">
