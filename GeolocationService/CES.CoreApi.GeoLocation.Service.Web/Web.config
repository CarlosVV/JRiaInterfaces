    <section name="dataCacheClients" type="Microsoft.ApplicationServer.Caching.DataCacheClientsSection, Microsoft.ApplicationServer.Caching.Core" allowLocation="true" allowDefinition="Everywhere" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="performanceLog" type="CES.CoreApi.Logging.Configuration.PerformanceLogConfiguration, CES.CoreApi.Logging, Version=1.0.0.0, Culture=neutral" />
    <section name="databasePerformanceLog" type="CES.CoreApi.Logging.Configuration.DatabasePerformanceLogConfiguration, CES.CoreApi.Logging, Version=1.0.0.0, Culture=neutral" />
    <section name="traceLog" type="CES.CoreApi.Logging.Configuration.TraceLogConfiguration, CES.CoreApi.Logging, Version=1.0.0.0, Culture=neutral" />
  </configSections>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5.2" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5.2" />
    <httpRuntime targetFramework="4.5" />
    <identity impersonate="false" />
  </system.web>
  <connectionStrings>
    <add name="Main" connectionString="Data Source=devdbmain.riadev.local;Initial Catalog=fxDB6;Persist Security Info=True;User ID=appUser_CoreAPI;Password=@ppC0r3ap1" providerName="System.Data.SqlClient" />
    <!--<add name="Main" connectionString="Data Source=(localdb)\Projects;Initial Catalog=Test;Persist Security Info=True;User ID=CoreAPI;Password=cAP!2014" providerName="System.Data.SqlClient" />-->
  </connectionStrings>
  <appSettings>
    <add key="ApplicationID" value="8000" />
    <add key="CacheName" value="CoreAPIDev" />
    <add key="CacheLifetime" value="0.00:10:00" />
  </appSettings>
  <!--
    isPerformanceLogEnabled - defines whether performance log enabled
    performanceLogThreshold - threshold in milliseconds, after which system logs invocation, 0 means log all calls 
    isPerformanceLogAsynchronous - defines whether performance log is asynchronous
  -->
  <performanceLog isPerformanceLogEnabled="true" performanceLogThreshold="0" isPerformanceLogAsynchronous="true" />
  <!--
    isDatabasePerformanceLogEnabled - defines whether database performance log enabled
    databasePerformanceLogThreshold - threshold in milliseconds, after which system logs invocation, 0 means log all calls 
    isDatabasePerformanceLogAsynchronous - defines whether database performance log is asynchronous
  -->
  <databasePerformanceLog isDatabasePerformanceLogEnabled="true" databasePerformanceLogThreshold="0" isDatabasePerformanceLogAsynchronous="true" />
  <!--
    isTraceLogEnabled - defines whether trace log enabled
    isTraceLogAsynchronous - defines whether trace log is asynchronous
  -->
  <traceLog isTraceLogEnabled="true" isTraceLogAsynchronous="true" isRequestLoggingEnabled="true" isResponseLoggingEnabled="true" />
  <dataCacheClients>
    <dataCacheClient name="default" isCompressionEnabled="true">
      <hosts>
        <host name="dntgssvcstest1.riadev.local" cachePort="22233" />
      </hosts>
      <!-- Production: mode="Transport" protectionLevel="EncryptAndSign" -->
      <securityProperties mode="None" protectionLevel="None" />
    </dataCacheClient>
  </dataCacheClients>
  <system.serviceModel>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="false" multipleSiteBindingsEnabled="false" minFreeMemoryPercentageToActivateService="0" />
    <diagnostics>
      <messageLogging logEntireMessage="true" logKnownPii="true" logMalformedMessages="true" logMessagesAtServiceLevel="true" logMessagesAtTransportLevel="true" maxMessagesToLog="300000" maxSizeOfMessageToLog="51200000" />
      <endToEndTracing propagateActivity="true" activityTracing="true" messageFlowTracing="true" />
    </diagnostics>
  </system.serviceModel>
  <system.diagnostics>
    <trace autoflush="true" />
    <sources>
      <source propagateActivity="true" name="System.ServiceModel" switchValue="Verbose,ActivityTracing">
        <listeners>
          <add type="System.Diagnostics.DefaultTraceListener" name="Default">
            <filter type="" />
          </add>
          <add name="xml">
            <filter type="" />
          </add>
        </listeners>
      </source>
      <source name="System.ServiceModel.MessageLogging">
        <listeners>
          <add type="System.Diagnostics.DefaultTraceListener" name="Default">
            <filter type="" />
          </add>
          <add name="xml">
            <filter type="" />
          </add>
        </listeners>
      </source>
      <source name="myUserTraceSource" switchValue="Verbose, ActivityTracing">
        <listeners>
          <add type="System.Diagnostics.DefaultTraceListener" name="Default">
            <filter type="" />
          </add>
          <add name="xml">
            <filter type="" />
          </add>
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add initializeData="C:\Logs\GeolocationWcfTrace.svclog" type="System.Diagnostics.XmlWriterTraceListener" name="xml">
        <filter type="" />
      </add>
    </sharedListeners>
  </system.diagnostics>
  <log4net>
    <root>
      <!-- Levels: OFF, DEBUG, INFO, NOTICE, WARN, ERROR, FATAL-->
      <level value="INFO" />
      <!--Log level priority in descending order:
      
      FATAL = 1  show  log -> FATAL 
      ERROR = 2  show  log -> FATAL ERROR 
      WARN =  3  show  log -> FATAL ERROR WARN 
      NOTICE = 4 show  log -> FATAL ERROR WARN NOTICE 
      INFO =  5  show  log -> FATAL ERROR WARN NOTICE INFO 
      DEBUG = 6  show  log -> FATAL ERROR WARN NOTICE INFO DEBUG -->
      <!--To write error log in file -->
      <appender-ref ref="ErrorLogFileAppender" />
      <!--To write error log to grey log -->
      <appender-ref ref="ErrorLogUdpAppender" />
      <!--To write trace log in file -->
      <appender-ref ref="TraceLogFileAppender" />
      <!--To write trace log to grey log -->
      <appender-ref ref="TraceLogUdpAppender" />
      <!--To write application performance messages to file -->
      <appender-ref ref="PerformanceLogFileAppender" />
      <!--To write application performance messages to grey log -->
      <appender-ref ref="PerformanceLogUdpAppender" />
      <!--To write application database performance messages to file -->
      <appender-ref ref="DatabasePerformanceLogFileAppender" />
      <!--To write application database performance messages to grey log -->
      <appender-ref ref="DatabasePerformanceLogUdpAppender" />
    </root>
    <appender name="ErrorLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\logs\CoreApi_Error_%date{yyyy-MM-dd}.txt" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%newline Log Time: [%date{yyyy-MM-dd HH:mm:ss.fff tt}] Thread#: [%thread] %newline% %m%n" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="ERROR" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="ErrorLogUdpAppender" type="gelf4net.Appender.GelfUdpAppender, Gelf4net">
      <remoteAddress value="10.30.8.6" />
      <remotePort value="12200" />
      <layout type="gelf4net.Layout.GelfLayout, Gelf4net">
        <param name="AdditionalFields" value="app:CoreAPI,service:Geolocation,logtype:ErrorLog,date:%date,thread:%thread,level:%level,logger:%logger" />
        <param name="ConversionPattern" value="%message" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="ERROR" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="TraceLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\logs\CoreApi_Trace_%date{yyyy-MM-dd}.txt" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%m%n" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="INFO" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="TraceLogUdpAppender" type="gelf4net.Appender.GelfUdpAppender, Gelf4net">
      <remoteAddress value="10.30.8.6" />
      <remotePort value="12200" />
      <threshold value="ON" />
      <layout type="gelf4net.Layout.GelfLayout, Gelf4net">
        <param name="AdditionalFields" value="app:CoreAPI,service:Geolocation,logtype:TraceLog,date:%date,thread:%thread,level:%level,logger:%logger" />
        <param name="ConversionPattern" value="%message" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="INFO" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="PerformanceLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\logs\CoreApi_Performance_%date{yyyy-MM-dd}.txt" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%m%n" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="WARN" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="PerformanceLogUdpAppender" type="gelf4net.Appender.GelfUdpAppender, Gelf4net">
      <remoteAddress value="10.30.8.6" />
      <remotePort value="12200" />
      <layout type="gelf4net.Layout.GelfLayout, Gelf4net">
        <param name="AdditionalFields" value="app:CoreAPI,service:Geolocation,logtype:PerformanceLog,date:%date,thread:%thread,level:%level,logger:%logger" />
        <param name="ConversionPattern" value="%message" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="WARN" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="DatabasePerformanceLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\logs\CoreApi_DBPerformance_%date{yyyy-MM-dd}.txt" />
      <layout type="log4net.Layout.PatternLayout">
