<?xml version="1.0" encoding="utf-8" ?>
<parameters>
	<parameter name="CompilationDebug" description="Compilation Debug" tags="" defaultvalue="__COMPILATIONDEBUG__">
		<parameterentry match="/configuration/system.web/compilation/@debug" scope="\\web.config$" kind="XmlFile" />
	</parameter>
	<parameter name="CacheName" description="AppFabric Cache Name" tags="" defaultvalue="__CACHENAME__">
		<parameterentry match="/configuration/appSettings/add[@key='CacheName']/@value" scope="\\web.config$" kind="XmlFile" />
	</parameter>
	<parameter name="AppFabricServers" description="AppFabric Servers" tags="" defaultvalue="__APPFABRICSERVERS__">
		<parameterentry match="/configuration/dataCacheClients/dataCacheClient/@hosts" scope="\\web.config$" kind="XmlFile" />
	</parameter>
	<parameter name="SvcLog" description="SvcLog File Location" tags="" defaultvalue="__SVCLOG__">
		<parameterentry match="/configuration/system.diagnostics/sharedListeners/add/@initializeData" scope="\\web.config$" kind="XmlFile" />
	</parameter>
	<parameter name="ErrorLog" description="Error Log File Location" tags="" defaultvalue="__ERRORLOG__">
		<parameterentry match="/configuration/log4net/appender[@name='ErrorLogFileAppender']/file/@value" scope="\\web.config$" kind="XmlFile" />
	</parameter>
	<parameter name="TraceLog" description="Trace Log File Location" tags="" defaultvalue="__TRACELOG__">
		<parameterentry match="/configuration/log4net/appender[@name='TraceLogFileAppender']/file/@value" scope="\\web.config$" kind="XmlFile" />
	</parameter>
	<parameter name="PerformanceLog" description="Performance Log File Location" tags="" defaultvalue="__PERFLOG__">
		<parameterentry match="/configuration/log4net/appender[@name='PerformanceLogFileAppender']/file/@value" scope="\\web.config$" kind="XmlFile" />
	</parameter>
	<parameter name="DBPerformanceLog" description="DB Performance Log File Location" tags="" defaultvalue="__DBPERFLOG__">
		<parameterentry match="/configuration/log4net/appender[@name='DatabasePerformanceLogFileAppender']/file/@value" scope="\\web.config$" kind="XmlFile" />
	</parameter>
	<parameter name="LogServerIP" description="Log Server IP Address" tags="" defaultvalue="__LOGSERVERIP__">
		<parameterentry match="/configuration/log4net/appender[@name='ErrorLogUdpAppender']/remoteAddress/@value" scope="\\web.config$" kind="XmlFile" />
		<parameterentry match="/configuration/log4net/appender[@name='TraceLogUdpAppender']/remoteAddress/@value" scope="\\web.config$" kind="XmlFile" />
		<parameterentry match="/configuration/log4net/appender[@name='PerformanceLogUdpAppender']/remoteAddress/@value" scope="\\web.config$" kind="XmlFile" />
		<parameterentry match="/configuration/log4net/appender[@name='DatabasePerformanceLogUdpAppender']/remoteAddress/@value" scope="\\web.config$" kind="XmlFile" />
	</parameter>
</parameters>
